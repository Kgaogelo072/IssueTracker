{
  "routes": [
    {
      "route": "/api/*",
      "rewrite": "https://issuetracker-api.azurewebsites.net/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "/assets/*",
      "/favicon.ico"
    ]
  }
}

