{
  "routes": [
    {
      "route": "/api/{*path}",
      "rewrite": "https://issuetracker-api.azurewebsites.net/api/{path}",
      "allowedRoles": ["anonymous"],
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/assets/*", "/*.{css,js,png,jpg,svg,ico,webp,woff,woff2}"]
  }
}
